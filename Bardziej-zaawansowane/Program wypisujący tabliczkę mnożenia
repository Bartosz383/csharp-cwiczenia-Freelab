using System;

namespace ConsoleApplication
{
    class Program
    {
        static void Main(string[] args)
        {
            for (int i = 1; i <= 10; i++)
            {
                // Pętla wypisująca mnożenie jednej liczby przez drugą
                for (int j = 1; j <= 10; j++)
                {
                    Console.WriteLine($"{i} x {j} = {i * j}");    
                }
                Console.WriteLine();    // Pusta linia, dla przejrzystości
            }
        }
    }
}
